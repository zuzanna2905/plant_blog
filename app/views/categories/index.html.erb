<div class='center'>
    <h1>Categories</h1>
    <%= will_paginate %>
    <% @categories.each { |category| %>
        <div class='row'>
            <ul class='well user col-md-4 col-md-offset-4'>
                <li><h4><%= link_to "#{category.name}", category_path(category) %></h4></li>
            </ul>
        </div>
    <%}%>
    <%= will_paginate %>
</div>